    <AssemblyTitle>App.Metrics.Middleware.Formatters.Json</AssemblyTitle>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <AssemblyName>App.Metrics.Middleware.Formatters.Json</AssemblyName>
    <PackageId>App.Metrics.Middleware.Formatters.Json</PackageId>
