{
  "version": "1.0.0-rtm",
  "description": "The library provides support for integrating ASP.NET 5 with the Metrics.NET Library",
  "authors": [ "Allan Hardy" ],
  "packOptions": {
    "releaseNotes": "Moved performance counter registration into a separate package (AspNet.Metrics.PerformanceCounters)",
    "tags": [ "mvc measurements charts timer histogram duration graphite influxdb health checks metrics" ],
    "repository": {
      "type": "git",
      "url": "git://github.com/alhardy/aspnet-metrics"
    },
    "projectUrl": "https://github.com/alhardy/aspnet-metrics",
    "licenseUrl": "https://github.com/alhardy/aspnet-metrics/blob/master/LICENSE",
    "iconUrl": "http://www.erata.net/Metrics.NET/metrics_32.png"
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Metrics.NET": "0.3.7",
    "Microsoft.AspNetCore.Mvc": "1.0.1",
    "Microsoft.AspNetCore.Routing": "1.0.1",
    "Microsoft.Extensions.DependencyInjection": "1.0.0",
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.0",
    "Microsoft.Extensions.Logging": "1.0.0",
    "Microsoft.Extensions.PlatformAbstractions": "1.0.0",
    "NETStandard.Library": "1.6.0",
    "System.Linq": "4.1.0"
  },
  "frameworks": {
    "net461": { }
  }
}
